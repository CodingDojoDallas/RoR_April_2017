<table >
  <tr>
    <th>Dojo Branch</th>
    <th>Street</th>
    <th>City</th>
    <th>State</th>
    <th>Actions</th>
  </tr>
  <% @dojos.each do |dojo| %>
    <tr>
      <td><%= dojo.branch %></td>
      <td><%= dojo.street %></td>
      <td><%= dojo.city %></td>
      <td><%= dojo.state %></td>
      <td>
        <a href="/dojo/<%= dojo.id %>">Show</a>
        <a href="/dojo/<%= dojo.id %>/edit" data-method="post">Edit</a>
        <a href="/dojo/<%= dojo.id %>/destroy" data-method="delete">Destroy</a>

      </td>
    </tr>
  <%end%>
</table>


<a href="/dojos/new">New Dojo</a>
