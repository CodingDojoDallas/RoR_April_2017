<div id="wrapper">
  <ul>
    <% flash.each do |key, value| %>
      <%= content_tag :li, value, class: "flash #{key}" %>
    <% end %>
  </ul>
  <div id="content">
    <h1><%= @dojo.branch %></h1>
    <p>Addres: <%= @dojo.street %></p>
    <p>City: <%= @dojo.city %> State: <%= @dojo.state %></p>
    <%= link_to "Home", root_path %> |
    <%= link_to "Edit", edit_dojo_path(@dojo) %>
  </div>
</div>
